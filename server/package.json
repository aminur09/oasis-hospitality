{
  "name": "oasis-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.js",
    "prisma:generate": "prisma generate --schema ../prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ../prisma/schema.prisma",
    "prisma:seed": "ts-node ../prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "argon2": "^0.30.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.0.0",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.0.0",
    "pino-http": "^10.0.0",
    "sanitize-html": "^2.12.1",
    "sharp": "^0.33.3",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prisma": "^5.18.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}